{
    "presets": [
        "@babel/env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-arrow-functions",
        [
            "import",
            {
                "libraryName": "@material-ui/core",
                "libraryDirectory": ".",
                "camel2DashComponentName": false
            }
        ]
    ]
}
